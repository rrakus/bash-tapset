  printf("-----------\n")
}
probe bash_command_if_call {
  printf("If command call:\n")
  printf("At line %d\n", line)
  printf("-----------\n")
}

probe bash_command_connection_call
{
  printf("Connection command call:\n")
  printf("At line %d\n", line)
  printf("command = %p\n", command)
  printf("connector = %d (%c)\n", connector, connector)
  printf("-----------\n")
}

